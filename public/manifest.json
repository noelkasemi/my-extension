{
  "manifest_version": 3,
  "name": "Phoenixtension",
  "version": "1.0",
  "description": "A simple Brave extension using React.",
  "permissions": ["history", "activeTab", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
